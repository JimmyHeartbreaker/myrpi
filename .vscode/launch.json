{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "remote target qemu",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/qemu-build/tmp-glibc/work/cortexa57-oe-linux/${fileBasenameNoExtension}/0.1-r0/image/usr/bin/${fileBasenameNoExtension}",
            "args": [],
            "cwd": "${workspaceFolder}/${relativeFileDirname}",
            "environment": [],
            "miDebuggerPath": "/usr/bin/gdb-multiarch",
            "miDebuggerServerAddress": "192.168.7.2:1234",
            "MIMode": "gdb",
            "targetArchitecture": "ARM64",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
          //  "preLaunchTask": "build_deploy"
        }
    ]
}