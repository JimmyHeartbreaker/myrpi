{
    "version": 4,
    "configurePresets": [
        {
            
            "name": "Target",
            "generator": "Ninja",
            "displayName": "GCC 11.3.0 aarch64-oe-linux",
            "description": "Cross compile with aarch64-oe-linux-gcc",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "cacheVariables": {
                "CMAKE_SYSTEM_PROCESSOR":"aarch64",
                "CMAKE_CXX_STANDARD 11":"11",
                "CMAKE_CXX_STANDARD_REQUIRED":"True",
                "KERNELHEADERS_DIR":"${sourceDir}/../../../../qemu-build/tmp-glibc/work-shared/qemuarm64/kernel-build-artifacts/",
                "KERNELHEADERS_INCLUDE_DIRS":"${sourceDir}/../../../../qemu-build/tmp-glibc/work-shared/qemuarm64/kernel-source/arch/arm64/include/",
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/install/${presetName}",
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_TOOLCHAIN_FILE": {
                    "type": "PATH",
                    "value": "${sourceDir}/../../../../qemu-build/conf/CMakeToolChain.qemuarm64.cmake"
                }
            }
        }
    ]
}